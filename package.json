{
  "name": "modular-o-mundo-adm",
  "version": "1.0.0",
  "description": "Sistema modular de administração",
  "scripts": {
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd o-mundo-a-sua-porta-frontend && npm start",
    "install:all": "npm install && cd backend && npm install && cd ../o-mundo-a-sua-porta-frontend && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}